<script>
    var navBar = document.getElementById("nav-bar");
    window.onscroll = displayNav;

    function displayNav() {
        var value = window.pageYOffset;
        if (value > 145) {
            navBar.style.opacity = 1;
            $('#containerFixMenu').css({opacity: 1});
        } else {
            navBar.style.opacity = 0;
            $('#containerFixMenu').css({opacity: 0});
        }
    }
